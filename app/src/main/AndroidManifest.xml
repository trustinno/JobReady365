<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.goldenictsolutions.win.jobready365_">


     
        
    <!-- Accessing camera hardware -->
        
    <uses-feature android:name="android.hardware.camera" />
     
        
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
       
     
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".Employee.JPolicy"
            android:configChanges="locale"
            android:label="@string/title_activity_jpolicy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Employee.JSignUpActivity"
            android:label="@string/title_activity_jsign_up"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Employee.JLoginActivity"
            android:label="@string/title_activity_jlogin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".employer.Emprcompro"
            android:label="CompanyProfile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".employer.Emprjobpost"
            android:label="@string/title_activity_emprjobpost"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Employee.JResumeActivity"
            android:label="@string/title_activity_jresume"
            android:theme="@style/AppTheme.NoActionBar"/>


        <!--
              android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"
        -->
        <activity
            android:name=".Employee.UploadActivity"
            android:label="@string/title_activity_upload"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>